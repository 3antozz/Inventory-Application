<% if (locals.errors) {%>
    <ul>
      <% errors.forEach(function(error) { %>
        <li><%= error.msg %></li>
      <% }); %>
    </ul>
<% } %>
<% if (locals.message) {%>
    <ul>
        <li><%= message %></li>
    </ul>
<% } %>
<% if (locals.validation) {%>
  <ul>
    <% validation.forEach(function(error) { %>
      <li><%= error %></li>
    <% }); %>
  </ul>
<% } %>